{
  "name": "mybatis-formatter",
  "displayName": "Mybatis Formatter",
  "description": "",
  "version": "0.0.2",
  "publisher": "cuongly99",
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:foo-lang"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "foo-lang",
        "aliases": [
          "Foo"
        ],
        "extensions": [
          "foo"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.format-foo",
        "title": "(Bad) Format Foo Files"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "sql-formatter": "^15.3.2",
    "xml-formatter": "^3.6.2"
  }
}
